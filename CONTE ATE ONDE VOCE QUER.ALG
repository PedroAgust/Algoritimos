algoritmo "Conte ate onde voce quer"
var
   Contador, AteOnde: inteiro
inicio
      Contador <- 0
      Escreval("Ate onde voce quer contar? ")
      Leia(AteOnde)
      Enquanto (contador <= AteOnde) faca
           Escreval(contador)
           Contador <- Contador + 1
      FimEnquanto
      Escreval("Terminei de contar")

fimalgoritmo